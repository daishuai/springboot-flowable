      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="edge-1729b37a-77ec-482c-ab8f-d637969ad67a" bpmnElement="notPassJointDispatch">
        <omgdi:waypoint x="940.0" y="285.0"/>
        <omgdi:waypoint x="1015.0" y="285.0"/>
        <omgdi:waypoint x="1090.0" y="284.99997"/>
